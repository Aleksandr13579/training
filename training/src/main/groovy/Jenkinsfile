def array1 = []
def array2 = [:]

pipeline {
    agent any

    options { timestamps() }

    parameters {
        string ( name: 'SERVER_IP', description: 'Enter your sereves ip address (delimiter is \" \" )' )
    }

    stages {
        stage('input') {
            steps {
                script {
                    arrayStr = "${STR}".split(',')
                    arrayStr.each { println(it.toString()) }
                }
                echo "${arrayStr}"
            }
        }
        stage('chekout') {
            steps {
                git url: 'https://github.com/Aleksandr13579/training.git', branch: '*/main'
                script {
                    println("${env.workDir}")
                }
            }
        }

        stage('two') {
            steps {
                echo 'hello'
            }
        }
    }
}